{
	"fields": {
		"accessible": {
			"fieldClass": "hideme"
		},
		"alarms": {
			"type": "table",
			"items": {
				"fields": {
					"Enabled": {
						"label": "{{ENABLED}}"
					},
					"Name": {
						"type": "text",
						"label": "{{ALARM_NAME}}",
						"placeholder": "{{MY_ALARM}}",
						"maxLength": 20
					},
					"WebTag": {
						"type": "text",
						"label": "{{WEB_TAGS}}",
						"placeholder": "<#ExtraTemp2 rc=y>"
					},
					"Type": {
						"type": "select",
						"label": "{{TYPE}}",
						"optionLabels": ["{{ABOVE}}", "{{BELOW}}", "{{EQUALS}}"],
						"sort": false,
						"removeDefaultNone": true
					},
					"Value": {
						"type": "number",
						"label": "{{VALUE}}",
						"placeholder": "5.5"
					},
					"Email": {
						"label": "{{EMAIL}}"
					},
					"EmailMsg": {
						"label": "{{EMAIL_MESSAGE}}",
						"placeholder": "{{MY_EMAIL}}"
					},
					"BskyFile": {
						"label": "{{BLUESKY_TEMPLATE}}",
						"placeholder": "web/BlueskyAlarm.txt"
					},
					"Latch": {
						"label": "{{LATCH}}"
					},
					"LatchHours": {
						"type": "number",
						"label": "{{LATCH_HOURS}}"
					},
					"Action": {
						"label": "{{ACTION}}",
						"placeholder": "./myscript.sh"
					},
					"ActionParams": {
						"label": "{{ACTION_PARAMS}}",
						"placeholder": "-s param1 -t <#ExtraTemp2>"
					}
				}
			},
			"actionbar": {
				"label": "Form actions",
				"actions": [{
					"action": "add",
					"enabled": true
				}, {
					"action": "remove",
					"enabled": true
				}, {
					"action": "up",
					"enabled": false
				}, {
					"action": "down",
					"enabled": false
				}]
			}
		}
	}
}